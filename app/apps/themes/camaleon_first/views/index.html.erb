<!-- +++++ Welcome Section +++++ -->
<div id="ww">
    <div class="container" >
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">
                <h2><%= raw current_theme.the_field("pd_name") %></h2>
                <h4><%= raw current_theme.the_field("pd_job_title") %></h4>
                <br/><br/>
                <div class="col-lg-12"</div>
                <%= raw current_theme.the_field("pd_description") %>
                </div>
              <div class="col-lg-6 top-buffer">
                <table class="table">
                  <tbody>
                    <tr>
                      <th style="width: 50%;">Name</th>
                      <td><%= raw current_theme.the_field("pd_name_on_table") %></td>
                    </tr>
                    <tr>
                      <th style="width: 50%;">Date of birth</th>
                      <td><%= raw current_theme.the_field("pd_dob") %></td>
                    </tr>
                  </tbody>
                </table> 
              </div>
              <div class="col-lg-6 top-buffer">
                <table class="table">
                  <tbody>
                    <tr>
                      <th style="width: 50%;">Email</th>
                      <td><%= raw current_theme.the_field("pd_email") %></td>
                    </tr>
                    <tr>
                      <th style="width: 50%;">Phone</th>
                      <td><%= raw current_theme.the_field("pd_phone") %></td>
                    </tr>
                  </tbody>
                </table> 
              </div>
            </div>
        </div>
        <hr/>
      
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">
                <h2>Professional Skills</h2>
                <br/><br/>
